    <parameter name="sdkVersion" value="4.4.2" /><!--安卓,IOS SDK版本-->
    <parameter name="appMainPackage" value="com.mdcn.mdonline" /><!--app主包名-->
    <parameter name="appActivity" value=".activity.WelcomeActivity" /> <!--app 主类名-->
    <parameter name="UserName" value="" /> <!-- 系统登录用户名-->
