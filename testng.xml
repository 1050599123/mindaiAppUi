    <listeners><!-- 监听器设置-->
        <listener class-name="org.webdriver.mindaiappui.utils.TestListener"/>
        <listener class-name="org.webdriver.mindaiappui.utils.TestReport"/>
    </listeners>
       <test name="登录失败测试：数据驱动">
        <classes>
            <class 
                name="org.webdriver.mindaiappui.cases.LoginTest">
            </class>
        </classes>
